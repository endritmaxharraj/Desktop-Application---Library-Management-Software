<VBox alignment="TOP_CENTER" minHeight="400.0" minWidth="800.0" onMouseDragged="#draged" onMousePressed="#pressed" prefHeight="400.0" prefWidth="800.0" spacing="25.0" styleClass="vBox" stylesheets="@../styles/admin.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.AdminController">
